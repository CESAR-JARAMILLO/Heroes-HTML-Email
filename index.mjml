    <mj-section background-color="black">
      <mj-column>
        <mj-image href="https://p2p.charityengine.net/2023thp?sm_guid=NzMwODgxfDMyNTQxNjIzfC0xfGNzYXJqYXJhbWlsbG84OUB5YWhvby5jb218Njg5OTQ1NHx8MHwwfDIxNTk0NzUyMnwxMDAwfDB8MHx8NzI0NjgwfDA1" width="354px" src="http://127.0.0.1:5500/images/heroes-logo.jpeg" alt="hero" />
      </mj-column>
